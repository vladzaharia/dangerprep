{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": ["NODE_ENV"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "env": ["NODE_ENV"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "deps:check": {
      "outputs": []
    },
    "deps:fix": {
      "cache": false,
      "outputs": []
    },
    "deps:update": {
      "cache": false,
      "outputs": []
    },
    "postinstall": {
      "cache": false,
      "outputs": []
    }
  },
  "globalDependencies": [
    "package.json",
    "turbo.json",
    "tsconfig.base.json",
    ".eslintrc.*",
    "prettier.config.*"
  ]
}
