{
  "name": "@dangerprep/cdn",
  "description": "High-performance self-hosted CDN for DangerPrep assets",
  "version": "1.0.0",
  "author": "Vlad Zaharia <hey@vlad.gg>",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@dangerprep/eslint": "workspace:*",
    "@dangerprep/prettier": "workspace:*",
    "@dangerprep/typescript": "workspace:*",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "assets",
    "cache",
    "cdn",
    "dangerprep",
    "performance",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "npx tsx src/server.ts",
    "docker:build": "docker compose build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:up": "docker compose up",
    "docker:up:build": "docker compose up --build",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json}",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "dist/server.d.ts"
}
