  "usage": {
    "html_example": "<!DOCTYPE html>\n<html>\n<head>\n  <link rel=\"stylesheet\" href=\"https://cdn.danger/fontawesome/css/all.min.css\">\n</head>\n<body>\n  <i class=\"fas fa-download\"></i> Download\n  <i class=\"fab fa-github\"></i> GitHub\n  <i class=\"far fa-heart\"></i> Like\n</body>\n</html>",
    "css_example": ".my-icon::before {\n  font-family: 'Font Awesome 6 Free';\n  content: '\\f019'; /* download icon */\n  font-weight: 900;\n}"
  },
  "icon_families": {
    "solid": "fas",
    "regular": "far", 
    "brands": "fab"
  },
  "tags": ["icons", "fonts", "svg", "css", "web-fonts"]
