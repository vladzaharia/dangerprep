{
  "name": "@dangerprep/portal",
  "description": "Modern React portal for DangerPrep hotspot services",
  "version": "1.0.0",
  "author": "Vlad Zaharia <hey@vlad.gg>",
  "dependencies": {
    "@awesome.me/webawesome": "3.0.0-beta.6",
    "@fontsource/aleo": "^5.2.7",
    "@fontsource/geist-mono": "^5.2.7",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "dockerode": "^4.0.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "node-cache": "^5.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.9.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@dangerprep/eslint": "workspace:*",
    "@dangerprep/prettier": "workspace:*",
    "@dangerprep/typescript": "workspace:*",
    "@types/dockerode": "^3.3.31",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jest": "^30.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.0.0"
  },
  "keywords": [
    "dangerprep",
    "hotspot",
    "portal",
    "react",
    "typescript",
    "vite",
    "webawesome"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.5.3",
  "private": true,
  "scripts": {
    "build": "tsc && vite build && node src/server/build.cjs",
    "clean": "rimraf dist build",
    "dev": "vite",
    "docker:build": "docker compose build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:up": "docker compose up",
    "docker:up:build": "docker compose up --build",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "start": "node src/server/production.cjs",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
